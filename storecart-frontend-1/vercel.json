{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "src/server.js" }
  ],
  "installCommand": "npm install && npx prisma generate",
  "buildCommand": "npm run build",
  "functions": {
  "src/**/*.js": {
    "includeFiles": ["node_modules/.prisma/client"]
  }
}
}
