{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "installCommand": "npm install && npx prisma generate",
  "buildCommand": "prisma migrate deploy",
  "functions": {
    "src/**/*.js": {
      "includeFiles": ["node_modules/.prisma/client/**"]
    }
  }
}
